Tab:AddToggle({
	Name = "Auto-Destroy NameTag",
	Default = false,
	Callback = function(Value)
		nameTog = Value
while nameTog do
-- Get the local player
local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer

-- Wait for the character to load
local character = workspace:WaitForChild(localPlayer.Name)

-- Function to destroy all BillboardGuis in the character
local function destroyBillboardGuis(parent)
    for _, child in ipairs(parent:GetDescendants()) do
        if child:IsA("BillboardGui") then
            child:Destroy()
        end
    end
end

-- Destroy BillboardGuis in the character
destroyBillboardGuis(character)

task.wait()
end
	end    
})

local Section = Tab:AddSection({
	Name = "At this point on, most would work but most are broken and soon to be fixed."
})

Tab:AddToggle({
	Name = "Anti-Slime",
	Default = false,
	Callback = function(Value)
sleme = Value
		while sleme do
local function destroyAllSoap()
    -- Get all descendants of the Workspace
    local descendants = game.Workspace:GetDescendants()
    
    -- Iterate through each descendant
    for _, descendant in ipairs(descendants) do
        -- Check if the name of the descendant is "soap"
        if descendant.Name == "Slime" then
            -- Check if the parent is not a Folder
            if not descendant:FindFirstAncestorWhichIsA("Folder") then
                -- Destroy the object
                descendant:Destroy()
            end
        end
    end
end

-- Call the function
destroyAllSoap()
task.wait(2)
end
	end    
})

