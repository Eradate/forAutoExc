local Tab = Window:MakeTab({
	Name = "Anti Toggles",
	Icon = "rbxassetid://7743871876",
	PremiumOnly = false
})

local position = Vector3.new(-8.89036465, 18.9488831, 5.74848366)
local rightVector = Vector3.new(0.442257434, 0, -0.896888137)
local upVector = rightVector:Cross(Vector3.new(1, 0, 0)).unit
local forwardVector = rightVector:Cross(upVector).unit
local orientation = CFrame.new(position, position + forwardVector, upVector)

-- Create the part
local partz = Instance.new("Part")
partz.Size = Vector3.new(2040, 30, 2040)  -- Set the size to 2040 studs in length, 1 stud in width
partz.CFrame = orientation  -- Set the part's CFrame
partz.Anchored = true  -- Anchor the part so it doesn't fall
partz.CanCollide = Value  -- Make the part non-collidable
partz.Transparency = 1  -- Make the part invisible

-- Parent the part to the workspace or another desired parent
partz.Parent = game.Workspace  -- Change 'game.Workspace' to another parent if needed a
	    

Tab:AddToggle({
	Name = "Anti Void",
	Default = false,
	Callback = function(Value)
	  Papz = Value
	  if Papz == true then
	   partz.CanCollide = true
	  else
		partz.CanCollide = false
	end
	end    
})

