	Default = false,
	Callback = function(Value)
		PuncH = Value
	  while PuncH do
	    local targets = {"Tree Lord", "Mushroom King", "Dog", "Cat"}
	    
	    for _, targetName in ipairs(targets) do
	        local target = workspace:FindFirstChild(targetName)
	        if target then
	            local args = {
	                [1] = target,
	                [2] = Vector3.new(22.601205825805664, 20.445518493652344, 25.27463150024414),
	                [3] = 2.8541431427001953,
	                [4] = target:FindFirstChild("Head") or target:FindFirstChild("Torso")
	            }
	            if args[4] then
	                game:GetService("ReplicatedStorage"):FindFirstChild("Remote Events").Punch:FireServer(unpack(args))
	            end
	        end
	    end
	    task.wait()
	  end
	end    
})

